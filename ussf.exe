using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace PCHS
{
    public partial class MyAccount : Form
    {
        string Username = "";
        public MyAccount(string User, string School1, string School2, string School3, string School4)
        {
            InitializeComponent();
            Username = User;
            lblUsername.Text = "Username: " + Username;
            textBox1.Text = School1;
            textBox2.Text = School2;
            textBox3.Text = School3;
            textBox4.Text = School4;
        }

        private void BtnEdit_Click(object sender, EventArgs e)
        {
            textBox1.ReadOnly = false;
            textBox2.ReadOnly = false;
            textBox3.ReadOnly = false;
            textBox4.ReadOnly = false;
            btnSave.Enabled = true;
        }

        private void BtnSave_Click(object sender, EventArgs e)
        {
            textBox1.ReadOnly = true;
            textBox2.ReadOnly = true;
            textBox3.ReadOnly = true;
            textBox4.ReadOnly = true;
            btnSave.Enabled = false;
        }

        private void BtnBack_Click(object sender, EventArgs e)
        {
            MainPage _mainPage = new MainPage(Username, null);
            this.Hide();
            _mainPage.ShowDialog();
        }
    }
}


