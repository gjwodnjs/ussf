School Account:

//////////////////////////////////////////////////////////////////////////////////

using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace PCHS
{
    public partial class SchoolAccount : Form
    {
        string SchoolName;
        string strTypeOfSchool = "NaN";
        bool PublicOrPrivate = false;
        bool Music = false;
        bool Robotics = false;
        bool Sports = false;
        bool Theater = false;
        bool Government = false;
        string User;
        string FullString = "";
        public SchoolAccount(string SchoolUsername)
        {
            InitializeComponent();
            lblWelcome.Text = "Welcome " + SchoolUsername;
        }

        private void Logic(object sender, EventArgs e)
        {
            //This will set the Type of School
            if (rdbElementary.Checked == true)
            {
                strTypeOfSchool = "Elementary School";
            }
            else if (rdbMiddleSchool.Checked == true)
            {
                strTypeOfSchool = "Middle School";
            }
            else if (rdbHighSchool.Checked == true)
            {
                strTypeOfSchool = "High School";
            }

            //This will set Public or Private
            if (rdbPublic.Checked == true)
            {
                PublicOrPrivate = true;
            }
            else if (rdbPrivate.Checked == true)
            {
                PublicOrPrivate = false;
            }

            if (chkGovernment.Checked == true)
            {
                Government = true;
            }

            if (chkMusic.Checked == true)
            {
                Music = true;
            }
            if (chkRobotics.Checked == true)
            {
                Robotics = true;
            }
            if (chkSports.Checked == true)
            {
                Sports = true;
            }
            if (chkTheater.Checked == true)
            {
                Theater = true;
            }
        }

        private void LocationText(object sender, EventArgs e)
        {
            txtLocation.Text = "";
            txtLocation.ForeColor = System.Drawing.Color.Black;
        }

        private void SchoolNameText(object sender, EventArgs e)
        {
            txtName.Text = "";
            txtName.ForeColor = System.Drawing.Color.Black;
        }
        string strPublicOrPrivate = null;
        string strExtracurricular = null;
        private void BtnSave_Click(object sender, EventArgs e)
        {
            SchoolName = txtName.Text;
            if (PublicOrPrivate == true)
            {
                strPublicOrPrivate = "Public_";
            }
            else
            {
                strPublicOrPrivate = "Private_";
            }
            if (Government == true)
                strExtracurricular += "Government_";
            if (Music == true)
                strExtracurricular += "Music_";
            if (Robotics == true)
                strExtracurricular += "Robotics_";
            if (Sports == true)
                strExtracurricular += "Sports_";
            if (Theater == true)
                strExtracurricular += "Theater_";
            FullString = SchoolName + "," + strPublicOrPrivate + strExtracurricular;
        }

        private void BtnLogOut_Click(object sender, EventArgs e)
        {
            AccountCreation accountCreation = new AccountCreation(FullString);
            this.Hide();
            accountCreation.ShowDialog();
        }
    }
}

