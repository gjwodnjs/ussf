Login:

//////////////////////////////////////////////////////////////////////////////////

using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace PCHS
{
    public partial class Login : Form
    {
        string LoginUser = "";
        string LoginPass = "";
        bool LoginParentOrSchool;
        string FullString;
        public Login(string LoginUsername, string LoginPassword, bool ParentOrSchool, string strFullString)
        {
            InitializeComponent();
            LoginUser = LoginUsername;
            LoginPass = LoginPassword;
            LoginParentOrSchool = ParentOrSchool;
            FullString = strFullString;
        }

        private void BtnLogin_Click(object sender, EventArgs e)
        {
            if (LoginParentOrSchool == true)
            {
                if (txtUsername.Text == LoginUser && txtPassword.Text == LoginPass)
                {
                    MainPage _mainPage = new MainPage(LoginUser, FullString);
                    this.Hide();
                    _mainPage.ShowDialog();
                }
            }
            else
            {
                SchoolAccount schoolAccount = new SchoolAccount(LoginUser);
                this.Hide();
                schoolAccount.ShowDialog();
            }

        }
    }
}


